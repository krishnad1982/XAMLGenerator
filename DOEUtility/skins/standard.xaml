        <!--Destination Template Content -->
        <ControlTemplate x:Key="DestinationInfo" TargetType="ContentControl">
            <Grid x:Name="DestinationGrid">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                    <RowDefinition Height="Auto"></RowDefinition>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="2*"></ColumnDefinition>
                    <ColumnDefinition Width="4*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="0" Grid.Column="0" Style="{StaticResource TextBlockLeftStyle}" Text="IOS:"/>
                <i:ComboBox Grid.Row="0" Grid.Column="1" x:Name="Ios" Style="{StaticResource BasicStyle}" IsEditable="False" 
                                ItemsSource="{Binding Ios.AvailableItems}" SelectedItem="{Binding Ios.Value, ValidatesOnDataErrors=True, ValidatesOnExceptions=True}" 
                                IsEnabled="{Binding Ios.IsEditable}" IsSorted="True" CloseDropDownOnTab="True"  VerticalAlignment="Top"/>
                <TextBlock Grid.Row="1" Grid.Column="0" Style="{StaticResource TextBlockLeftStyle}" Text="Destination:"/>
                <i:DestinationComboBox Grid.Row="1" Grid.Column="1" x:Name="Destination" Style="{StaticResource BasicStyle}" IsEditable="True"  
                                ItemsSource="{Binding Destination.AvailableItems}" SelectedItem="{Binding Destination.Value, ValidatesOnDataErrors=True, ValidatesOnExceptions=True}" 
                                IsEnabled="{Binding Destination.IsEditable}" IsSorted="True" CloseDropDownOnTab="True"  VerticalAlignment="Top"/>
                <TextBlock x:Name="TxtVersion" Grid.Row="2" Grid.Column="1" Text="" VerticalAlignment="Bottom" HorizontalAlignment="Right" Style="{StaticResource BasicStyle}"></TextBlock>
            </Grid>
        </ControlTemplate>
        <!--End Of Destination Template Content -->

        <!--Market Details Template Content -->
        <ControlTemplate TargetType="ContentControl" x:Key="MarketInfo">
            <Grid Style="{StaticResource GridMarketDataStyle}" x:Name="StockGrid">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="8*"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="6*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <!--<TextBlock Grid.Row="0" Grid.Column="0" Text="Instrument:" Style="{StaticResource TextBlockMarketLeftStyle}"/>-->
                <TextBlock Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="0" Style="{StaticResource TextBlockMarketOfferStyle}" Text="{Binding SecurityDetail.Description}"/>
                <TextBlock Grid.Column="0" Grid.Row="1" Style="{StaticResource TextBlockMarketLeftStyle}" Text="Bid Price:"/>
                <TextBlock Grid.Column="1" Grid.Row="1" Style="{StaticResource TextBlockMarketDataStyle}" Text="{Binding QuoteDetail.BidPrice}"/>
                <TextBlock Grid.Column="0" Grid.Row="2" Style="{StaticResource TextBlockMarketLeftStyle}" Text="Bid Volume:"/>
                <TextBlock Grid.Column="1" Grid.Row="2" Style="{StaticResource TextBlockMarketDataStyle}" Text="{Binding QuoteDetail.BidVolume}"/>
                <TextBlock Grid.Column="0" Grid.Row="3" Style="{StaticResource TextBlockMarketLeftStyle}" Text="Last Trade Price:"/>
                <TextBlock Grid.Column="1" Grid.Row="3" Style="{StaticResource TextBlockMarketDataStyle}" Text="{Binding QuoteDetail.LastPrice}"/>

                <TextBlock Grid.Column="2" Grid.Row="0" Style="{StaticResource TextBlockMarketLeftStyle}" Text="ExchangeTime:"/>
                <TextBlock Grid.Column="3" Grid.Row="0" Style="{StaticResource TextBlockMarketDataStyle}" Text="{Binding QuoteDetail.ExchangeTime}"/>
                <TextBlock Grid.Column="2" Grid.Row="1" Style="{StaticResource TextBlockMarketOfferStyle}" Text="{Binding QuoteDetail.AskPrice}"/>
                <TextBlock Grid.Column="3" Grid.Row="1" Style="{StaticResource TextBlockMarketStyle}" Text="Offer Price"/>
                <TextBlock Grid.Column="2" Grid.Row="2" Style="{StaticResource TextBlockMarketOfferStyle}" Text="{Binding QuoteDetail.AskVolume}"/>
                <TextBlock Grid.Column="3" Grid.Row="2" Style="{StaticResource TextBlockMarketStyle}" Text="Offer Volume"/>
                <TextBlock Grid.Column="2" Grid.Row="3" Style="{StaticResource TextBlockMarketLeftStyle}" Text="Last Delta Price:"/>
                <TextBlock Grid.Column="3" Grid.Row="3" Style="{StaticResource TextBlockMarketDataStyle}" Text="{Binding QuoteDetail.LastPriceDelta}"/>
            </Grid>
        </ControlTemplate>
        <!--End Of Market Details Template Content-->

        <!--Side Template Content -->
        <ControlTemplate TargetType="ContentControl" x:Key="SideInfo">
            <Grid Background="{i:SideBrushBinding Side.Code}" x:Name="SideGrid">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>
                <ItemsControl Grid.Column="0" Grid.Row="0" KeyboardNavigation.DirectionalNavigation="Contained" KeyboardNavigation.IsTabStop="False" KeyboardNavigation.TabNavigation="Once" Style="{StaticResource BasicStyle}">
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                            </Grid>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                    <ItemsControl.BindingGroup>
                        <Binding Path="Side.Code" ValidatesOnDataErrors="True" ValidatesOnExceptions="True"/>
                    </ItemsControl.BindingGroup>

                    <i:RadioButton Background="{i:SideBrushBinding Source={x:Static i:SideCodes.Buy}}" Content="_Buy" Grid.Column="0" Grid.Row="0" IsChecked="{Binding Side.Code, ConverterParameter={x:Static i:SideCodes.Buy}, Converter={StaticResource EqualsConverter}}" Style="{StaticResource SideSelectButtonStyle}" x:Name="SideBuy">
                        <d:RadioButton.IsEnabled>
                            <MultiBinding Converter="{StaticResource LogicalAndConverter}">
                                <Binding Converter="{StaticResource ContainsConverter}" ConverterParameter="{x:Static i:SideCodes.Buy}" Path="Side.AvailableItems"/>
                                <Binding Path="Side.IsEditable"/>
                            </MultiBinding>
                        </d:RadioButton.IsEnabled>
                    </i:RadioButton>
                    <i:RadioButton Background="{i:SideBrushBinding Source={x:Static i:SideCodes.Sell}}" Content="_Sell" Grid.Column="0" Grid.Row="1" IsChecked="{Binding Side.Code, ConverterParameter={x:Static i:SideCodes.Sell}, Converter={StaticResource EqualsConverter}}" Style="{StaticResource SideSelectButtonStyle}" x:Name="SideSell">
                        <d:RadioButton.IsEnabled>
                            <MultiBinding Converter="{StaticResource LogicalAndConverter}">
                                <Binding Converter="{StaticResource ContainsConverter}" ConverterParameter="{x:Static i:SideCodes.Sell}" Path="Side.AvailableItems"/>
                                <Binding Path="Side.IsEditable"/>
                            </MultiBinding>
                        </d:RadioButton.IsEnabled>
                    </i:RadioButton>
                </ItemsControl>
            </Grid>
        </ControlTemplate>
        <!--End Of Side Template Content -->

        <!--Order Details Template Content -->
        <ControlTemplate TargetType="ContentControl" x:Key="OrderInfo">
            <Grid x:Name="OrderGrid">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="2*"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="2*"/>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Column="0" Grid.Row="0" Style="{StaticResource TextBlockStyle}" Text="Instrument Code:" x:Name="LblCode"/>
                <i:SecurityPicker Grid.Column="1" Grid.Row="0" Security="{Binding Security.Value, ValidatesOnDataErrors=True, ValidatesOnExceptions=True}" Style="{StaticResource BasicStyle}" i:InitialFocus.IsEnabled="True" x:Name="Security">
                    <w:Interaction.Triggers>
                        <w:EventTrigger EventName="LostFocus">
                            <i:InvokeCommandAction Command="{Binding BroadcastSecurity}" CommandParameter="True" IsEnabled="{Binding Form.Settings.IsBroadcastSecurityEnabled}"/>
                            <i:InvokeCommandAction Command="{Binding ShowMarketDepth}" CommandParameter="True" IsEnabled="{Binding Form.Settings.IsShowMarketDepthEnabled}"/>
                        </w:EventTrigger>
                    </w:Interaction.Triggers>
                    <i:SecurityPicker.IsEnabled>
                        <MultiBinding Converter="{StaticResource LogicalAndConverter}">
                            <Binding Converter="{StaticResource EqualsConverter}" ConverterParameter="0" Path="BuyLeg.ParentOrderNumber"/>
                            <Binding Converter="{StaticResource EqualsConverter}" ConverterParameter="0" Path="SellLeg.ParentOrderNumber"/>
                            <Binding Path="Security.IsEditable"/>
                        </MultiBinding>
                    </i:SecurityPicker.IsEnabled>
                </i:SecurityPicker>
                <TextBlock Grid.Column="0" Grid.Row="1" Style="{StaticResource TextBlockStyle}" Text="Volume:" x:Name="LblLimitPrice"/>
                <i:NumericUpDown AcceptsMultiplierSuffix="True" DisplayFormat="0" Grid.Column="1" Grid.Row="1" Increment="{Binding Path=VolumeIncrement.Value, TargetNullValue=100}" IncrementCount="{Binding Path=VolumeIncrementCount.Value}" IsEnabled="{Binding Volume.IsEditable}" Minimum="0" RoundingDecimalPlaces="0" Style="{StaticResource BasicStyle}" Value="{Binding Volume.Value, ValidatesOnExceptions=True, ValidatesOnDataErrors=True}" i:Skin.IsVolume="True" x:Name="Volume"/>
                <TextBlock Grid.Column="2" Grid.Row="0" Style="{StaticResource TextBlockStyle}">Limit Price:</TextBlock>
                <i:NumericUpDown DisplayFormat="{Binding Source={StaticResource PriceFormat}, Path=StringFormat}" Grid.Column="3" Grid.Row="0" Increment="{Binding Path=PriceIncrement.Value, TargetNullValue=1}" IncrementCount="{Binding Path=PriceIncrementCount.Value}" Minimum="0" RoundingDecimalPlaces="{Binding Source={StaticResource PriceFormat}, Path=MaxDecimalPlaces}" i:Skin.IsPrice="True" x:Name="Price">
                    <i:NumericUpDown.Style>
                        <Style BasedOn="{StaticResource BasicStyle}" TargetType="i:NumericUpDown">
                            <Setter Property="Value" Value="{Binding Path=Price.Value, ValidatesOnExceptions=True, ValidatesOnDataErrors=True}"/>
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding Path=Price.IsAvailable}" Value="False">
                                    <Setter Property="Value" Value="{Binding Path=Price.EstimatedPrice, Mode=OneWay}"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </i:NumericUpDown.Style>
                </i:NumericUpDown>
                <TextBlock Grid.Column="2" Grid.Row="1" Style="{StaticResource TextBlockStyle}" Text="Account:" x:Name="LBlAccount"/>
                <i:AccountComboBox AutomationProperties.AutomationId="AccountCode" Grid.Column="3" Grid.Row="1" IsEditable="True" IsEnabled="{Binding AccountCode.IsEditable}" IsSorted="True" ItemsSource="{Binding AccountCode.AvailableItems}" SelectedItem="{Binding AccountCode.Value, ValidatesOnExceptions=True, ValidatesOnDataErrors=True}" Style="{StaticResource BasicStyle}" Text="{Binding AccountCode.Value, ValidatesOnExceptions=True, ValidatesOnDataErrors=True}" x:Name="Account" Ios="{Binding Ios.Value}"/>

            </Grid>
        </ControlTemplate>
        <!--End Of Order Details Template Content-->

        <!--Footer Template Content -->
        <ControlTemplate TargetType="ContentControl" x:Key="FooterInfo">
            <Grid Name="ActionButtonsGrid">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition SharedSizeGroup="Buttons"/>
                    <ColumnDefinition SharedSizeGroup="Buttons"/>
                    <ColumnDefinition SharedSizeGroup="Buttons"/>
                </Grid.ColumnDefinitions>
                <Button Command="{Binding Form.Send}" CommandParameter="{StaticResource SendAndClose}" Content="Send &amp; Close" Grid.Column="0" IsDefault="{Binding Form.Settings.DefaultSendAction, Converter={StaticResource EqualsConverter}, ConverterParameter={x:Static i:SendAction.SendAndClose}}" Style="{StaticResource ButtonBaseStyle}" x:Name="SendAndClose"/>
                <Button Command="{Binding Form.Send}" CommandParameter="{StaticResource SendAndClear}" Content="Send" Grid.Column="1" IsDefault="{Binding Form.Settings.DefaultSendAction, Converter={StaticResource EqualsConverter}, ConverterParameter={x:Static i:SendAction.SendAndClear}}" Style="{StaticResource ButtonBaseStyle}" x:Name="Send"/>
                <Button Command="{Binding Form.Close}" Content="Close" Grid.Column="2" IsCancel="True" Style="{StaticResource ButtonBaseStyle}" x:Name="Close"/>
            </Grid>
        </ControlTemplate>
        <!--End Of Footer Template Content-->
    </i:Skin.Resources>

    <!--Algo Parameter Details -->
    <ScrollViewer Focusable="False" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
        <StackPanel Background="White" Focusable="False" MaxWidth="{Binding Width, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type i:DynamicOEComponent}}}" MinWidth="{i:FontWidth 95}" x:Name="MainPanel">

            <!--Header Template Implementation-->
            <Grid x:Name="SubHeaderGrid">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>
                <GroupBox Grid.Column="0" Grid.Row="0" Header="Trading Destination" Style="{StaticResource GroupBoxStyle}" x:Name="Trade">
                    <Grid x:Name="SubTradeGrid">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <ContentControl Focusable="False" Grid.Column="0" Grid.Row="0" Template="{StaticResource LogoInfo}" x:Name="LogoControl"/>
                        <ContentControl Focusable="False" Grid.Column="1" Grid.Row="0" Template="{StaticResource DestinationInfo}" x:Name="DestControl"/>
                    </Grid>
                </GroupBox>
                <GroupBox Grid.Column="1" Grid.Row="0" Header="Market Details" Style="{StaticResource GroupBoxStyle}" VerticalAlignment="Top" x:Name="Market">
                    <ContentControl Focusable="False" Template="{StaticResource MarketInfo}" x:Name="MarketControl"/>
                </GroupBox>
            </Grid>
            <!--End Of Header Template Implementation-->

            <!--Order Template Implementation-->
            <Grid x:Name="SubOrderGrid">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="3*"/>
                </Grid.ColumnDefinitions>
                <GroupBox Grid.Column="0" Grid.Row="0" Header="Side" Style="{StaticResource SideInfoGroupBoxStyle}" x:Name="Side">
                    <ContentControl Focusable="False" Template="{StaticResource SideInfo}" x:Name="SideControl"/>
                </GroupBox>
                <GroupBox Grid.Column="1" Grid.Row="0" Header="Order Details" Style="{StaticResource GroupBoxStyle}" x:Name="OrderDetails">
                    <ContentControl Focusable="False" Template="{StaticResource OrderInfo}" x:Name="OrderControl"/>
                </GroupBox>
            </Grid>
            <!--End Of Order Template Implementation-->

            <!--Algo Details Starting-->
            <GroupBox Header="Timer Algo Details" KeyboardNavigation.TabNavigation="Local" Style="{StaticResource GroupBoxStyle}" x:Name="AlgoDetails">
                <Grid x:Name="AlgoGrid">
                    
                </Grid>
            </GroupBox>
            <!--Ending Algo Details-->

            <!--Footer Template Implementation-->
            <ContentControl Focusable="False" Template="{StaticResource FooterInfo}" x:Name="FooterControl"/>
            <!--End Of Footer Template Implementation-->

        </StackPanel>
    </ScrollViewer>
    <!--End Of Algo Parameter Details -->
</i:Skin>